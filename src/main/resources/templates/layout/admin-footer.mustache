<script>
const sideWrap = document.querySelector('.admin-side');
const adminMenuBtn = document.querySelector('.admin-menu-btn');
function mobileSideOn(){
    if(sideWrap.classList.contains('on')){
        sideWrap.classList.remove('on')
    }else{
        sideWrap.classList.add('on')
    }
}
function mobileSideOff(){
    sideWrap.classList.remove('on')
}
function headerEvent(){
  var winWidth = window.innerWidth;
  if(winWidth > 1200){
    mobileSideOff();
  }else if(winWidth > 768){
  }else if(winWidth <= 768){
    adminMenuBtn.addEventListener('click',function(){
        mobileSideOn();
    });
  }
}
headerEvent()
window.onresize = () => {
  setTimeout(function(){headerEvent()},300);
}

</script>
